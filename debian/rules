#!/usr/bin/make -f

export PYBUILD_NAME=statichcpd
export PYBUILD_BEFORE_TEST=cp -r mypystubs {build_dir}/statichcpd
export PYBUILD_AFTER_TEST=rm -rf {build_dir}/statichcpd/mypystubs

%:
	dh $@ --with python3 --buildsystem=pybuild

override_dh_installsystemd:
	dh_installsystemd

override_dh_installinit:
	# dh_installsystemd fails to make the service enabled without this
	dh_installinit
